02babf56d501"],

Cell[TextData[{
 "f(x) = h(x ; ",
 StyleBox["N, n, k",
  FontSlant->"Italic"],
 ") = P { x successes in a random sample of size ",
 StyleBox["n",
  FontSlant->"Italic"],
 " taken without replacement from a population of size ",
 StyleBox["N",
  FontSlant->"Italic"],
 " containing ",
 StyleBox["k",
  FontSlant->"Italic"],
 " successes}\n\n",
 StyleBox["f(x) =  h(x ; ",
  FontSize->18],
 StyleBox["N, n, k",
  FontSize->18,
  FontSlant->"Italic"],
 StyleBox[") = ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"(", GridBox[{
        {"k"},
        {"x"}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"N", " ", "-", "  ", "k"}]},
        {
         RowBox[{"n", " ", "-", " ", "x"}]}
       }], ")"}]}], 
    RowBox[{"(", GridBox[{
       {"N"},
       {"n"}
      }], ")"}]], TraditionalForm]],
  FontSize->18,ExpressionUUID->"dbb655a1-b9ef-400f-b4a0-57ebac9a3a37"],
 "\n\n\t   for x = 0, 1, ..., ",
 StyleBox["k",
  FontSlant->"Italic"],
 " \t",
 StyleBox["if k < n or",
  FontSlant->"Italic"],
 "\n\t\tx= 0, 1, ..., ",
 StyleBox["n\t if n < k. \n",
  FontSlant->"Italic"],
 "For the hypergeometric r.v. X, E[X] = ",
 Cell[BoxData[
  FormBox[
   FractionBox["nk", "N"], TraditionalForm]],ExpressionUUID->
  "11d76576-447d-4be5-9f38-55da99296b60"],
 ", and Var[X] =\n\t",
 StyleBox["n",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   FractionBox["k", "N"], TraditionalForm]],
  FontSize->18,ExpressionUUID->"024ab393-618e-4a09-a3d0-e1597bc22a5c"],
 StyleBox["(1 - ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   FractionBox["k", "N"], TraditionalForm]],
  FontSize->18,ExpressionUUID->"a8b0a01e-5c64-45d0-9535-bc1eac12ecaa"],
 StyleBox[")( ",
  FontSize->18],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"N", "-", "n"}], 
      RowBox[{"N", "-", "1"}]], ")"}], "."}], TraditionalForm]],
  FontSize->18,ExpressionUUID->"b43abe04-a931-4d33-9f8b-0adef367dfcb"]
}], "Text",
